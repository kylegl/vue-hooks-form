{
  "name": "@kylegl/vue-hooks-form-zod",
  "version": "0.0.15-beta.1",
  "description": "zod resolver for vue-hooks-form",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vue-hooks-form#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylegl/vue-hooks-form.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/vue-hooks-form/issues"
  },
  "keywords": [
    "zod",
    "vue-hooks-form",
    "vue-hooks-form-resolver"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts"
  },
  "peerDependencies": {
    "@kylegl/vue-hooks-form-core": "workspace:*"
  },
  "dependencies": {
    "@kylegl/vue-hooks-form-core": "workspace:*",
    "zod": "3.22.4"
  }
